{
    "name": "tazya/gendiff",
    "description": "Cli application for search differences in configuration files",
    "type": "project",
    "version": "0.0.1",
    "keywords": ["tool", "hexlet", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Chernyshev",
            "email": "vengador32@gmail.com"
        }
    ],
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/cli.php",
            "src/differ.php",
            "src/parsers.php",
            "src/Formatters/pretty.php",
            "src/Formatters/plain.php",
            "src/Formatters/json.php"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpunit/phpunit": "^9.1"
    },
    "scripts": {
	    "phpcs": "phpcs"
    }
}
